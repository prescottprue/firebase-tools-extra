{
  "name": "firebase-tools-extra",
  "version": "0.0.7",
  "description": "Extra methods for firebase-tools which support using a service account.",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "es/index.js",
  "bin": {
    "firebase-extra": "./bin/firebase-tools-extra"
  },
  "files": [
    "bin",
    "cmds",
    "lib",
    "es"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "watch": "npm run build:commonjs -- --watch",
    "watch:es": "npm run build:es -- --watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run clean && npm run build",
    "prepush": "npm run lint"
  },
  "keywords": [
    "firebase-tools",
    "firebase",
    "cli"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "firebase-admin": "^6.4.0",
    "lodash": "^4.17.11",
    "npx": "^10.2.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "rimraf": "^2.6.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/firebase-tools-extra.git"
  },
  "bugs": {
    "url": "https://github.com/prescottprue/firebase-tools-extra/issues"
  },
  "author": {
    "name": "Prescott Prue",
    "url": "https://github.com/prescottprue"
  }
}
